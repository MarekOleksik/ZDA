
public class Faktura {
	private static int numerFaktury;
	private static String data;
	private static Double cena;
	private static int ilosc;
	
	public Faktura(int numerFaktury, String data, Double cena, int ilosc)
	{
		this.numerFaktury = numerFaktury;
		this.data = data;
		this.cena = cena;
		this.ilosc = ilosc;
		
	}
	

}
