
public class Produkty {
	private static String kodProduktu;
	private static String numerProduktu;

	public Produkty() {
	};

	public Produkty(String kodProduktu, String numerProduktu) {
		this.kodProduktu = kodProduktu;
		this.numerProduktu = numerProduktu;
	}

	public String getNumerProduktu() {
		return numerProduktu;
	}

	public String getKodProduktu() {
		return kodProduktu;
	}
}
