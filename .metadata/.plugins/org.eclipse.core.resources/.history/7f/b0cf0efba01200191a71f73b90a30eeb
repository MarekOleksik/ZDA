
public class Faktura extends Produkty{
	private static int numerFaktury;
	private static String data;
	private static Double cena;
	private static int ilosc;
	
	public Faktura(String numerProduktu, String kodProduktu, int numerFaktury, String data, Double cena, int ilosc)
	{
		super(numerProduktu, kodProduktu);
		this.numerFaktury = numerFaktury;
		this.data = data;
		this.cena = cena;
		this.ilosc = ilosc;
		
	}
	

}
