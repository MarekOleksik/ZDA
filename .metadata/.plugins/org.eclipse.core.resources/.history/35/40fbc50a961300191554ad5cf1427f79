
public class Faktura extends Produkty {
	private static int numerFaktury;
	private static String data;
	private static Double cena;
	private static int ilosc;

	public Faktura(String data, int numerFaktury, String nazwaProduktu, String kodProduktu, Double cena, int ilosc) {
		super(nazwaProduktu, kodProduktu);
		this.numerFaktury = numerFaktury;
		this.data = data;
		this.cena = cena;
		this.ilosc = ilosc;
	}
	public Faktura(){}
	public int getNumerFaktury()
	{
		return numerFaktury;
	}
	public String getData()
	{
		return data;
	}
	public Double getCena()
	{
		return cena;
	}
	public int getIlosc()
	{
		return ilosc;
	}

}
